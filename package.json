{
  "name": "luxewash-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html --chrome-flags='--headless'",
    "test:mobile": "npm run build && npm run start & sleep 5 && npm run lighthouse && pkill -f 'next start'",
    "generate:pages": "npx tsx scripts/generate-pages.ts",
    "update:sitemap": "npx tsx scripts/update-sitemap.ts",
    "seo:full": "npm run generate:pages && npm run update:sitemap",
    "fix:nearby-cities": "node scripts/fix-nearby-cities-placeholder.js",
    "optimize:images": "node scripts/optimize-images.js",
    "seo:optimize": "npm run optimize:images && npm run update:sitemap",
    "build:production": "npm run seo:optimize && npm run build",
    "analyze": "ANALYZE=true npm run build",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.11",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.20",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.511.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.33.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "glob": "^11.0.3",
    "jsdom": "^26.1.0",
    "natural": "^8.1.0",
    "tailwindcss": "^4",
    "tailwindcss-fluid-type": "^2.0.7",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5"
  }
}
